<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.learning.course.dao.CertificateMapper">

    <select id="listByUserName" resultType="com.learning.course.entity.Certificate">
        select * from certificate where user_name = #{username}
    </select>
</mapper>
